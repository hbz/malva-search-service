-Dlog4j.path=${dir}/../var/log/malva-search-service
-Dlog4j.level=debug
-Dio.netty.recycler.maxCapacity=0
-Dwebapp.home=${dir}/../var/lib/malva-search-service
-Dwebapp.profile=hap
-Dwebapp.home=/var/lib/malva-search-service
-Dwebapp.extension.elasticsearch.transport.enabled=true
-Dwebapp.extension.elasticsearch.transport.cluster=hap
-Dwebapp.extension.elasticsearch.transport.host.0=10.3.2.81:9300
-Dwebapp.extension.elasticsearch.transport.host.1=10.3.2.83:9300
-Dwebapp.extension.elasticsearch.transport.host.2=10.3.2.85:9300
